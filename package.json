{
  "name": "hone",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "copyViteManifestForDev": "cp -r dist/.vite public",
    "build": "tsc && vite build && npm run copyViteManifestForDev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "fonts": "node scripts/localFonts.js",
    "docker": "npm run fonts && npm run build && docker build -t hone-server ."
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.79",
    "@mlc-ai/web-runtime": "^0.18.0-dev2",
    "@mlc-ai/web-tokenizers": "^0.1.6",
    "decent-portal": "^1.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sl-spiel": "^1.0.5",
    "wouter": "^3.7.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.326",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@webgpu/types": "^0.1.61",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^3.1.4"
  }
}
